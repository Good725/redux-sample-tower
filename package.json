{
  "name": "redux-tower",
  "version": "0.0.2",
  "description": "Saga powered routing engine for Redux app.",
  "homepage": "https://github.com/kuy/redux-tower",
  "authors": [
    "Yuki Kodama <endflow.net@gmail.com>"
  ],
  "keywords": [
    "redux",
    "react",
    "router",
    "routing",
    "redux-saga",
    "saga"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuy/redux-tower.git"
  },
  "main": "./lib/index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --progress --content-base examples/",
    "build": "NODE_ENV=production ./node_modules/.bin/babel src --out-dir lib --ignore __tests__",
    "build:examples": "NODE_ENV=production ./node_modules/.bin/webpack",
    "test": "ava",
    "flow": "./node_modules/.bin/flow",
    "clean": "./node_modules/.bin/rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "history": "^4.5.0",
    "querystring": "^0.2.0",
    "ruta3": "^2.0.1"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-remove-console": "^6.8.0",
    "babel-plugin-unassert": "^2.1.2",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.37.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-logger": "^2.7.4",
    "redux-saga": "^0.14.0",
    "rimraf": "^2.5.4",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.63.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.27",
    "webpack-dev-server": "^2.1.0-beta.12"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}
